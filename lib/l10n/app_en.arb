{
  "@@locale": "en",
  "appTitle": "Travel Assistant",
  "@appTitle": {
    "description": "The title of the application"
  },
  "travelFormStepTitle": "Step {stepNumber}",
  "@travelFormStepTitle": {
    "description": "Title for a step in the travel form, includes step number placeholder",
    "placeholders": {
      "stepNumber": {
        "type": "int",
        "example": "1"
      }
    }
  },
  "departureAirportStepTitle": "Where are you flying from?",
  "@departureAirportStepTitle": {
    "description": "Title for the departure airport selection step"
  },
  "departureAirportHintText": "Search departure airport (e.g., IST, Istanbul)",
  "@departureAirportHintText": {
    "description": "Hint text for the departure airport search field"
  },
  "arrivalAirportStepTitle": "Where are you flying to?",
  "@arrivalAirportStepTitle": {
    "description": "Title for the arrival airport selection step"
  },
  "arrivalAirportHintText": "Search arrival airport (e.g., JFK, New York)",
  "@arrivalAirportHintText": {
    "description": "Hint text for the arrival airport search field"
  },
  "travelDatesStepTitle": "When are you travelling?",
  "@travelDatesStepTitle": {
    "description": "Title for the travel dates selection step"
  },
  "selectDatesButtonLabel": "Select Travel Dates",
  "@selectDatesButtonLabel": {
    "description": "Button label to open the date range picker"
  },
  "selectedDatesLabel": "Selected: {startDate} - {endDate}",
  "@selectedDatesLabel": {
    "description": "Label displaying the selected start and end dates",
    "placeholders": {
      "startDate": {
        "type": "String",
        "example": "2024-07-15"
      },
      "endDate": {
        "type": "String",
        "example": "2024-07-20"
      }
    }
  },
  "noDatesSelected": "No dates selected",
  "@noDatesSelected": {
    "description": "Text shown when no travel dates have been selected yet"
  },
  "nationalityStepTitle": "What is your nationality?",
  "@nationalityStepTitle": {
    "description": "Title for the nationality selection step"
  },
  "nationalityHintText": "Search your nationality (e.g., US, Turkey)",
  "@nationalityHintText": {
    "description": "Hint text for the nationality search field"
  },
  "selectedNationalityLabel": "Selected: {nationalityName}",
  "@selectedNationalityLabel": {
    "description": "Label displaying the selected nationality",
    "placeholders": {
      "nationalityName": {
        "type": "String",
        "example": "United States"
      }
    }
  },
  "validationErrorNationalityMissing": "Please select your nationality.",
  "@validationErrorNationalityMissing": {
    "description": "Validation error if nationality is not selected"
  },
  "errorInvalidDateRange": "End date cannot be before start date.",
  "@errorInvalidDateRange": {
    "description": "Error message for invalid date range selection"
  },
  "validationErrorDepartureAirportMissing": "Please select a departure airport.",
  "@validationErrorDepartureAirportMissing": {
    "description": "Validation error if departure airport is not selected"
  },
  "validationErrorArrivalAirportMissing": "Please select an arrival airport.",
  "@validationErrorArrivalAirportMissing": {
    "description": "Validation error if arrival airport is not selected"
  },
  "validationErrorDateRangeMissing": "Please select travel dates.",
  "@validationErrorDateRangeMissing": {
    "description": "Validation error if travel dates are not selected"
  },
  "selectedAirportLabel": "Selected: {airportName} ({airportCode})",
  "@selectedAirportLabel": {
    "description": "Label displayed when an airport is selected, showing its name and code",
    "placeholders": {
      "airportName": {
        "type": "String",
        "example": "Istanbul Airport"
      },
      "airportCode": {
        "type": "String",
        "example": "IST"
      }
    }
  },
  "navigationPrevious": "Previous",
  "@navigationPrevious": {
    "description": "Button text to go to the previous step"
  },
  "navigationNext": "Next",
  "@navigationNext": {
    "description": "Button text to go to the next step"
  },
  "navigationSubmit": "Get Travel Plan",
  "@navigationSubmit": {
    "description": "Button text to submit the form and get travel plan"
  },
  "submittingForm": "Submitting form...",
  "@submittingForm": {
    "description": "Message shown when the form is being submitted"
  }
} 